[
    {
        "Id": "cpu_time_raw",
        "MetricStat": {
            "Metric": {
                "Namespace": "NathanielRN/aws-otel-python-soaking-tests-demo/soak-tests-",
                "MetricName": "process.cpu.time",
                "Dimensions": [
                    {
                        "Name": "process.command_line",
                        "Value": "/usr/local/bin/python3 application.py"
                    }
                ]
            },
            "Stat": "Sum",
            "Period": 600
        },
        "Label": "CPU Time Raw",
        "ReturnData": false
    },
    {
        "Id": "cpu_load_expr",
        "Expression": "cpu_time_raw/PERIOD(cpu_time_raw)/2*100",
        "Label": "CPU Load Percentage for 2 CPUs",
        "ReturnData": true,
        "Period": 600
    }
]
